    - CurrSpeaker: 1
      OptionalNpcReference: {fileID: 11400000, guid: ee1da375ae4db704d92072d1a5229c06,
        type: 2}
      Text: Mr. {LastName}, what are you doing here?
      CustomNpcImageOptions:
      - {fileID: 2800000, guid: 2a6ca242a5b5ec6429e14333da197408, type: 3}
      CustomBackground: {fileID: 2800000, guid: 61b64585fd866e24abdb56454bb4236a,
