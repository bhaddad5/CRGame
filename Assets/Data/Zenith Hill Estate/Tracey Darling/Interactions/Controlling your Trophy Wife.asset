        yPos: 0
        width: 0
    - CurrSpeaker: 1
      OptionalNpcReference: {fileID: 0}
      Text: Tracey, you ARE my Trophy Wife.  You'll wear the necklace proudly.
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 0}
      CustomBackgroundNpcLayout:
        xPos: 0
        yPos: 0
        width: 0
    - CurrSpeaker: 0
      OptionalNpcReference: {fileID: 0}
      Text: I won't!
