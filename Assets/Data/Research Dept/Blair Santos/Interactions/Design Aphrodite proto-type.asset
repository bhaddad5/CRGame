      OptionalNpcReference: {fileID: 11400000, guid: 4f5b0cef4033d884badd26e5c47e6af2,
        type: 2}
      Text: Damn it, damn it, damn it!
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: 0cfc0bf2b9b5fe8418cc4c3bd0caceb4,
        type: 3}
