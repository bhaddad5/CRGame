      OptionalNpcReference: {fileID: 11400000, guid: 7c902c9082b20154aa5efc973815811c,
        type: 2}
      Text: Stephanie Tremont is the COO, right?
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: 1ff9ec7b19ed8564f83c2e483f5d71a3,
        type: 3}
