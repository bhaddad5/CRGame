      OptionalNpcReference: {fileID: 11400000, guid: 7a7caa08786bd91498605c6b78933ce1,
        type: 2}
      Text: The red line there?  Yes, yes-- I go!
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: cfb7623fd11365945929e2e4093b29da,
        type: 3}
      CustomBackgroundNpcLayout:
        xPos: 0.5
        yPos: 0.3
