  Result:
    Dialogs:
    - CurrSpeaker: 0
      OptionalNpcReference: {fileID: 11400000, guid: 7bb3db0059160694ab763a593176fba8,
        type: 2}
      Text: Initiate, a word please.
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: d4c380f749165114ea117aee532acf10,
        type: 3}
      CustomBackgroundNpcLayout:
        xPos: 0.5
