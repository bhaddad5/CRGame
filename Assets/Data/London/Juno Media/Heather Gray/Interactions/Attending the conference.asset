    - CurrSpeaker: 1
      OptionalNpcReference: {fileID: 11400000, guid: 5b372237565c8c845afe56c63b5c1019,
        type: 2}
      Text: 'Hello, Ms. Gray.  It looks like you''re heading out.  '
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: 1e5a63f8ee680eb4b91a79fd7353a9f1,
        type: 3}
      CustomBackgroundNpcLayout:
        xPos: 0.5
