    - CurrSpeaker: 0
      OptionalNpcReference: {fileID: 11400000, guid: bc33bf02957c7994b940aa4e96ab4303,
        type: 2}
      Text: 'Zarasa, Mr. {LastName} here has suggested Cromwell Intellysys may be
        projecting a frumpy image! '
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: 2e42676c53ec1054da4afa5642b81def,
        type: 3}
      CustomBackgroundNpcLayout:
        xPos: 0.5
