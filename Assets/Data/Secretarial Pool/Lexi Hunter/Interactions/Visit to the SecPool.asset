        yPos: 0
        width: 0
    - CurrSpeaker: 1
      OptionalNpcReference: {fileID: 0}
      Text: 'No.  We''re here for something more educational.  I''d like you to show
        the boys here how you train our office girls.  Select a trouble maker and
        we''ll watch you correct her.   '
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 0}
      CustomBackgroundNpcLayout:
        xPos: 0
        yPos: 0
        width: 0
    - CurrSpeaker: 0
