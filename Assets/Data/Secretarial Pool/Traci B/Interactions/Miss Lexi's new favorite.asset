      CustomBackground: {fileID: 0}
      CustomBackgroundNpcLayout:
        xPos: 0
        yPos: 0
        width: 0
    - CurrSpeaker: 0
      OptionalNpcReference: {fileID: 0}
      Text: 'I hope so, Mr. {LastName}!  Miss Lexi is like my Big Sister-- I admire
        her so much!  She''s my mentor and I don''t want to let her down by misbehaving!  '
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 0}
      CustomBackgroundNpcLayout:
        xPos: 0
        yPos: 0
        width: 0
    - CurrSpeaker: 1
      OptionalNpcReference: {fileID: 0}
      Text: 'Very good!  Tell Miss Lexi she has permission to fraternize with you
        outside the office.  I hope you don''t mind being the ''Other Woman'' as
        she is a married lady.  I hope you two have fun together.    '
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 0}
