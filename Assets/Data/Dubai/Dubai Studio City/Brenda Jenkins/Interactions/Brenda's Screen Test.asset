        type: 2}
      Text: The creatives enjoyed Jo Beth's screen test-- she did a great job.  You
        must be very proud!
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 0}
      CustomBackgroundNpcLayout:
        xPos: 0
        yPos: 0
        width: 0
      OptionalAudioClip: {fileID: 0}
