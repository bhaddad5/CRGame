        width: 350
      OptionalAudioClip: {fileID: 0}
      OptionalStartMusicClip: {fileID: 0}
    - CurrSpeaker: 0
      OptionalNpcReference: {fileID: 11400000, guid: 1c3d35c3205bb3e43878d098e4027282,
        type: 2}
      Text: Sheikh Jubur anticipated this and has already paid for your accommodations! 
        Please enjoy your time here-- the Burj is now your home in Dubai, Mr. {LastName}! 
        I hope your suite is to your satisfaction?
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: d6f0c8ce396bed24d9a9cc16b39d5aa6,
        type: 3}
      CustomBackgroundNpcLayout:
        xPos: 0.5
