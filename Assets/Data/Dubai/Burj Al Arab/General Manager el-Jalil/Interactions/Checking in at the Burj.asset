    - CurrSpeaker: 1
      OptionalNpcReference: {fileID: 11400000, guid: 1c3d35c3205bb3e43878d098e4027282,
        type: 2}
      Text: It is... magnificent!
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: d6f0c8ce396bed24d9a9cc16b39d5aa6,
        type: 3}
      CustomBackgroundNpcLayout:
        xPos: 0.5
