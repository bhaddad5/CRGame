    - CurrSpeaker: 1
      OptionalNpcReference: {fileID: 11400000, guid: 1c3d35c3205bb3e43878d098e4027282,
        type: 2}
      Text: 'I can''t accept such largesse-- I insist on paying my own way.   '
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: 2b8c97d9c3fc9b44eb1bf5c25032ef65,
        type: 3}
      CustomBackgroundNpcLayout:
        xPos: 0.5
