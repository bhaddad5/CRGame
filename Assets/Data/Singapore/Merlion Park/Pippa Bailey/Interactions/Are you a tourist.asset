        width: 450
      OptionalAudioClip: {fileID: 0}
      OptionalStartMusicClip: {fileID: 0}
    - CurrSpeaker: 1
      OptionalNpcReference: {fileID: 11400000, guid: fc98898d6bb80004f81bda6ede6e3051,
        type: 2}
      Text: What's that, Pippa?
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: 786326520bcd1b0458dc9201a0f26f43,
        type: 3}
      CustomBackgroundNpcLayout:
        xPos: 0.5
