    Dialogs:
    - CurrSpeaker: 0
      OptionalNpcReference: {fileID: 0}
      Text: Mr. {LastName}!  I trust your stay with us has been a pleasurable one? 
        If not, how may I make it so?
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 0}
      CustomBackgroundNpcLayout:
