    Dialogs:
    - CurrSpeaker: 0
      OptionalNpcReference: {fileID: 0}
      Text: Congratulations on your promotion to Executive!  Now that you've succesfully
        completed your Management Trainee program, we need to develop a PDP for you.
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 0}
      CustomBackgroundNpcLayout:
