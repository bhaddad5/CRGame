    - CurrSpeaker: 0
      OptionalNpcReference: {fileID: 11400000, guid: 3963f50d6424e1649a4b75504bf00eb6,
        type: 2}
      Text: How wonderful!  What did you have in mind?
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: 4d1814875e75a754fac574bf562d5251,
        type: 3}
      CustomBackgroundNpcLayout:
        xPos: 0.45
