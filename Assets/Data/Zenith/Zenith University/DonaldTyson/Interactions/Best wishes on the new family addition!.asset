    - CurrSpeaker: 1
      OptionalNpcReference: {fileID: 11400000, guid: 49c059802b4b2a842be5a5c55f4435f6,
        type: 2}
      Text: You mentioned you might be looking for an au pair.  I may have a candidate.
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: 7f239f2c1dffed941ad3536b63160463,
        type: 3}
      CustomBackgroundNpcLayout:
        xPos: 0.5
