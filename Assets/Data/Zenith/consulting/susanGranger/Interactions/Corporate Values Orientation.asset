    Dialogs:
    - CurrSpeaker: 0
      OptionalNpcReference: {fileID: 11400000, guid: 2d2d9fe634f1ea843a4402dfeb8ad59a,
        type: 2}
      Text: All new hires are required to attend Corporate Values seminars.  It is
        part of your HR Orientation Program.
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: 83dfbc2aa65584948afbc9c10added39,
        type: 3}
      CustomBackgroundNpcLayout:
        xPos: 0.35
