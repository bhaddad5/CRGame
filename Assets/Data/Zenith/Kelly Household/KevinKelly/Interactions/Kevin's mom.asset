      OptionalNpcReference: {fileID: 11400000, guid: fa1076b1698cb75439943b545976ce7d,
        type: 2}
      Text: Hey Kevin!  How's it going?
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: 9b4f6ecb9a68e124fbc2baedff7d3f5b,
        type: 3}
      CustomBackgroundNpcLayout:
