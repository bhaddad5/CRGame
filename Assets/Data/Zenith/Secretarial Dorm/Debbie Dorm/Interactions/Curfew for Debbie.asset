        width: 400
      OptionalAudioClip: {fileID: 0}
      OptionalStartMusicClip: {fileID: 0}
    - CurrSpeaker: 1
      OptionalNpcReference: {fileID: 11400000, guid: e919a2647617e8146b5be552b475f8ce,
        type: 2}
      Text: 'If she''s going to behave this way, she needs a curfew.  Early Lights
        Out for the little bitch until she learns her lesson! '
      CustomNpcImageOptions:
      - {fileID: 2800000, guid: 1d7fa578ba525bb4abed41e664019e2c, type: 3}
      CustomBackground: {fileID: 0}
      CustomBackgroundNpcLayout:
        xPos: 0.5
        yPos: 0.3
        width: 400
      OptionalAudioClip: {fileID: 0}
      OptionalStartMusicClip: {fileID: 0}
    - CurrSpeaker: 1
      OptionalNpcReference: {fileID: 11400000, guid: 14df74e6844c717478de06c275bd6470,
        type: 2}
      Text: But it's still light out!  You're sending me to bed now-- in the middle
        of the day?
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 0}
      CustomBackgroundNpcLayout:
        xPos: 0.5
        yPos: 0.3
        width: 400
      OptionalAudioClip: {fileID: 8300000, guid: 30610dab00450dc42aea681d94bc3573,
        type: 3}
      OptionalStartMusicClip: {fileID: 0}
    - CurrSpeaker: 1
      OptionalNpcReference: {fileID: 11400000, guid: 14df74e6844c717478de06c275bd6470,
        type: 2}
      Text: Get ready for bed, Debbie.  Miss LIsa has given you an order!
