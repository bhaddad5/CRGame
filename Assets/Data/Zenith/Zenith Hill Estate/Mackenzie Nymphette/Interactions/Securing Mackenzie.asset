      CustomBackgroundNpcLayout:
        xPos: 0.5
        yPos: 0.4
        width: 300
      OptionalAudioClip: {fileID: 0}
      OptionalStartMusicClip: {fileID: 0}
    - CurrSpeaker: 0
      OptionalNpcReference: {fileID: 0}
      Text: Fine.  I don't care!  It doesn't bother me at all!
      CustomNpcImageOptions:
      - {fileID: 2800000, guid: 30e4b1f52a5009548929252959ad4527, type: 3}
      CustomBackground: {fileID: 2800000, guid: 26af887ce2611244e8504d6437577c96,
