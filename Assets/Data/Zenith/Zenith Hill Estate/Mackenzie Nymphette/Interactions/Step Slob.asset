    - CurrSpeaker: 1
      OptionalNpcReference: {fileID: 11400000, guid: 4d7bbd9e5d8ecee4dbaec1cb452fe01e,
        type: 2}
      Text: Right.  Where is she?
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: 26af887ce2611244e8504d6437577c96,
        type: 3}
      CustomBackgroundNpcLayout:
        xPos: 0.5
        yPos: 0.4
        width: 300
      OptionalAudioClip: {fileID: 0}
      OptionalStartMusicClip: {fileID: 0}
    - CurrSpeaker: 0
      OptionalNpcReference: {fileID: 11400000, guid: 4d7bbd9e5d8ecee4dbaec1cb452fe01e,
        type: 2}
      Text: "You\u2019re not going to tell her I told you, right?"
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: 26af887ce2611244e8504d6437577c96,
        type: 3}
      CustomBackgroundNpcLayout:
        xPos: 0.5
        yPos: 0.4
        width: 300
      OptionalAudioClip: {fileID: 8300000, guid: 22c9844f3d2e4754bac68c9281266f31,
        type: 3}
      OptionalStartMusicClip: {fileID: 0}
    - CurrSpeaker: 1
      OptionalNpcReference: {fileID: 11400000, guid: 4d7bbd9e5d8ecee4dbaec1cb452fe01e,
        type: 2}
      Text: Mackenzie!
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: 26af887ce2611244e8504d6437577c96,
        type: 3}
      CustomBackgroundNpcLayout:
        xPos: 0.5
        yPos: 0.4
        width: 300
      OptionalAudioClip: {fileID: 0}
      OptionalStartMusicClip: {fileID: 0}
    - CurrSpeaker: 0
      OptionalNpcReference: {fileID: 11400000, guid: 4d7bbd9e5d8ecee4dbaec1cb452fe01e,
        type: 2}
      Text: "OK, ok!  She\u2019s squirreled up in the Parlour Room in the Guest House. 
        As soon as you leave for work, she sometimes goes there and watches teevee
        and grazes on ice cream for the whole day.  Mostly sappy romances and trashy
        reality tv stuff."
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: 26af887ce2611244e8504d6437577c96,
        type: 3}
      CustomBackgroundNpcLayout:
        xPos: 0.5
        yPos: 0.4
        width: 300
      OptionalAudioClip: {fileID: 8300000, guid: da0b24dbf97684c4cb56bc914b68d531,
        type: 3}
      OptionalStartMusicClip: {fileID: 0}
    - CurrSpeaker: 1
      OptionalNpcReference: {fileID: 11400000, guid: 4d7bbd9e5d8ecee4dbaec1cb452fe01e,
        type: 2}
      Text: "You\u2019re joking!"
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: 26af887ce2611244e8504d6437577c96,
        type: 3}
      CustomBackgroundNpcLayout:
        xPos: 0.5
        yPos: 0.4
        width: 300
      OptionalAudioClip: {fileID: 0}
      OptionalStartMusicClip: {fileID: 0}
    - CurrSpeaker: 0
      OptionalNpcReference: {fileID: 11400000, guid: 4d7bbd9e5d8ecee4dbaec1cb452fe01e,
        type: 2}
      Text: "I wish I was, Daddy!  Debbie is a real slob when you\u2019re not around. 
        Aside from ordering the maid around, she\u2019s just kind of a lazy pig.  "
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: 26af887ce2611244e8504d6437577c96,
        type: 3}
      CustomBackgroundNpcLayout:
        xPos: 0.5
        yPos: 0.4
        width: 300
      OptionalAudioClip: {fileID: 8300000, guid: 6e1ad7ade8104a04bbb8f55d65388f32,
        type: 3}
      OptionalStartMusicClip: {fileID: 0}
    - CurrSpeaker: 1
      OptionalNpcReference: {fileID: 11400000, guid: 4d7bbd9e5d8ecee4dbaec1cb452fe01e,
        type: 2}
      Text: Show me.
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: 26af887ce2611244e8504d6437577c96,
        type: 3}
      CustomBackgroundNpcLayout:
        xPos: 0.5
        yPos: 0.4
        width: 300
      OptionalAudioClip: {fileID: 0}
      OptionalStartMusicClip: {fileID: 0}
    OptionalPopups:
    - Title: Mackenzie leads you to the Guest House.
      Videos:
      - {fileID: 32900000, guid: 2473b08874c99e1469515042a1aae2e0, type: 3}
      Textures: []
      DialogClips:
      - {fileID: 8300000, guid: e1430d64bf9e59a40a7ec8ae2b85ca17, type: 3}
      Text: "Unless you put a stop to this, Debbie won\u2019t be fit company!"
    - Title: '"See, Daddy?"'
      Videos:
