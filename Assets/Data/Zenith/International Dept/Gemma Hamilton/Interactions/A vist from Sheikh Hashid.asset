      OptionalNpcReference: {fileID: 11400000, guid: 6729792a737019a46831d21ca98c56ba,
        type: 2}
      Text: My Lord!  What an honor!  We are so pleased you decided to take up our
        offer of hospitality!
      CustomNpcImageOptions: []
