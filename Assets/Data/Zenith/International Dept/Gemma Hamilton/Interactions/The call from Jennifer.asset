      OptionalAudioClip: {fileID: 0}
      OptionalStartMusicClip: {fileID: 8300000, guid: 1adcbce5878371c4c92b404c3cd24473,
        type: 3}
    - CurrSpeaker: 1
      OptionalNpcReference: {fileID: 11400000, guid: d248b00fc853b4845a5da195f269d404,
        type: 2}
      Text: How?
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: 9ef55447bce818949932d5e32e92a35c,
        type: 3}
      CustomBackgroundNpcLayout:
        xPos: 0.45
        yPos: 0.36
        width: 340
      OptionalAudioClip: {fileID: 0}
      OptionalStartMusicClip: {fileID: 0}
    - CurrSpeaker: 0
      OptionalNpcReference: {fileID: 11400000, guid: d248b00fc853b4845a5da195f269d404,
        type: 2}
      Text: 'You don''t want to be bothered with all the minutiae.  Let me handle
        that for you.  Wouldn''t it be useful for you to start getting a lot of wins
        from International-- right after you''ve taken over? '
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: 9ef55447bce818949932d5e32e92a35c,
        type: 3}
      CustomBackgroundNpcLayout:
        xPos: 0.45
        yPos: 0.36
        width: 340
      OptionalAudioClip: {fileID: 0}
      OptionalStartMusicClip: {fileID: 0}
    - CurrSpeaker: 1
      OptionalNpcReference: {fileID: 11400000, guid: d248b00fc853b4845a5da195f269d404,
        type: 2}
      Text: That would be most useful.  But wouldn't that make you look bad?
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: 9ef55447bce818949932d5e32e92a35c,
        type: 3}
      CustomBackgroundNpcLayout:
        xPos: 0.45
        yPos: 0.36
        width: 340
      OptionalAudioClip: {fileID: 0}
      OptionalStartMusicClip: {fileID: 0}
    - CurrSpeaker: 0
      OptionalNpcReference: {fileID: 11400000, guid: d248b00fc853b4845a5da195f269d404,
        type: 2}
      Text: 'I already look bad.  All I want is a stable, safe position that offers
        a working visa.  If you keep me on, I''ll make you look like an expert at
        International. '
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: 9ef55447bce818949932d5e32e92a35c,
        type: 3}
      CustomBackgroundNpcLayout:
        xPos: 0.45
        yPos: 0.36
        width: 340
      OptionalAudioClip: {fileID: 0}
      OptionalStartMusicClip: {fileID: 0}
    - CurrSpeaker: 2
      OptionalNpcReference: {fileID: 11400000, guid: d248b00fc853b4845a5da195f269d404,
        type: 2}
      Text: 'NOTE: You agree to keep Gemma on-- for now.  When she stops performing,
        you''ll consider a different outcome for her-- and her daughter Holly.'
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: 9ef55447bce818949932d5e32e92a35c,
        type: 3}
