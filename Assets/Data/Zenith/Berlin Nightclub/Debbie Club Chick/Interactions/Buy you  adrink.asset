    - CurrSpeaker: 0
      OptionalNpcReference: {fileID: 11400000, guid: 8d78b5bb64aca49438dd8dc9f780b77c,
        type: 2}
      Text: Sorry, Mr. {LastName}-- I'm not on your time-- I'm on my OWN time, ok? 
        I'm not leaving!
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: 8afb77f136546d94a8324c2c41fb21a8,
        type: 3}
      CustomBackgroundNpcLayout:
        xPos: 0.5
