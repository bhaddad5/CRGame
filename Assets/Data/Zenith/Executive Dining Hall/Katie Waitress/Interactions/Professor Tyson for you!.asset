    Dialogs:
    - CurrSpeaker: 0
      OptionalNpcReference: {fileID: 11400000, guid: 532f58f85b2abd84cb0d657a4fa3cec2,
        type: 2}
      Text: Professor Tyson for you, Sir!
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: f0bf6d1683bf9aa4faaa11a2eb7effbf,
        type: 3}
      CustomBackgroundNpcLayout:
