    Dialogs:
    - CurrSpeaker: 0
      OptionalNpcReference: {fileID: 11400000, guid: 5e669a1c0e002604c952078aa61bea6d,
        type: 2}
      Text: Mr. {LastName}!  Debbie, I see you brought your boss to help you shop. 
        I always encourage that with secretaries!
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: 0e6402f5c5dd12641ae4a04c6591f9a0,
