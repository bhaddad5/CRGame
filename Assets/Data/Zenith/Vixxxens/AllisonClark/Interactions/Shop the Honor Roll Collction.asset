    Dialogs:
    - CurrSpeaker: 0
      OptionalNpcReference: {fileID: 11400000, guid: ee1da375ae4db704d92072d1a5229c06,
        type: 2}
      Text: Mr. {LastName}, we have so much work to do!  Should we be shopping in
        the middle of the day like this?
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: 0e6402f5c5dd12641ae4a04c6591f9a0,
        type: 3}
      CustomBackgroundNpcLayout:
        xPos: 0.6
