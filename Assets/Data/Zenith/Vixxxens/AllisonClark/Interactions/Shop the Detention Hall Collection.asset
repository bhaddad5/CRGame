    Dialogs:
    - CurrSpeaker: 0
      OptionalNpcReference: {fileID: 11400000, guid: 5e669a1c0e002604c952078aa61bea6d,
        type: 2}
      Text: Mr. {LastName}-- and Debbie!  How can I help?  Looking for anything special?
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: 0e6402f5c5dd12641ae4a04c6591f9a0,
        type: 3}
      CustomBackgroundNpcLayout:
