    - CurrSpeaker: 0
      OptionalNpcReference: {fileID: 11400000, guid: 8d9f38aa47f1c4f48b97afffaa769f0d,
        type: 2}
      Text: Please let us go, Mr. {LastName}.
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: 85b8dd32509540d4faf282e6b4772bff,
        type: 3}
      CustomBackgroundNpcLayout:
        xPos: 0.6
