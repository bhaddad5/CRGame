      OptionalNpcReference: {fileID: 11400000, guid: 524c352db1c37b24fa4df8930133125e,
        type: 2}
      Text: You looked tired and troubled my boy!
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: 61d5c60fc2d8d2743bf89ac2ee391663,
        type: 3}
      CustomBackgroundNpcLayout:
