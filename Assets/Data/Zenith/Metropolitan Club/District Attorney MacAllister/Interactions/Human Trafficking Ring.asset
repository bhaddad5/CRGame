      OptionalNpcReference: {fileID: 11400000, guid: 011424d9b1f7ff148b7ac7aa8d0483ee,
        type: 2}
      Text: '{FirstName}!  How''s my favorite campaign donor!'
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: 61d5c60fc2d8d2743bf89ac2ee391663,
        type: 3}
      CustomBackgroundNpcLayout:
