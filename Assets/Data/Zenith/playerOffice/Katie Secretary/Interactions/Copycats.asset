    - CurrSpeaker: 0
      OptionalNpcReference: {fileID: 11400000, guid: 2030f38a3a781734280cf9b1d4bfecea,
        type: 2}
      Text: And who are you, my dear?
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: 68299baf120e84f4eb3579f5864df440,
        type: 3}
      CustomBackgroundNpcLayout:
        xPos: 0.52
