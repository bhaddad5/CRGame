      OptionalAudioClip: {fileID: 0}
      OptionalStartMusicClip: {fileID: 0}
    - CurrSpeaker: 0
      OptionalNpcReference: {fileID: 11400000, guid: ab87a85575fcd92479d099fd2eccee18,
        type: 2}
      Text: "It wouldn\u2019t have taken a lot to keep him but, hey\u2014 I assumed
        you felt differently about it.  You\u2019re the head guy, right?  It is ultimately
        your call.  I did send you that urgent memo\u2014"
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: 68299baf120e84f4eb3579f5864df440,
        type: 3}
      CustomBackgroundNpcLayout:
        xPos: 0.52
        yPos: 0.42
        width: 375
      OptionalAudioClip: {fileID: 0}
      OptionalStartMusicClip: {fileID: 0}
    - CurrSpeaker: 1
      OptionalNpcReference: {fileID: 11400000, guid: ab87a85575fcd92479d099fd2eccee18,
        type: 2}
      Text: I never got any memo!
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: 68299baf120e84f4eb3579f5864df440,
        type: 3}
      CustomBackgroundNpcLayout:
        xPos: 0.52
        yPos: 0.42
        width: 375
      OptionalAudioClip: {fileID: 0}
      OptionalStartMusicClip: {fileID: 0}
    - CurrSpeaker: 0
      OptionalNpcReference: {fileID: 11400000, guid: ab87a85575fcd92479d099fd2eccee18,
        type: 2}
      Text: "Oh, damn!  I sent it to Debbie and then talked to her about it!  You
        were out of town.  She said she\u2019d make it a priority to get your response. 
        I can\u2019t believe it!  You never got the memo?"
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: 68299baf120e84f4eb3579f5864df440,
        type: 3}
      CustomBackgroundNpcLayout:
        xPos: 0.52
        yPos: 0.42
        width: 375
      OptionalAudioClip: {fileID: 0}
      OptionalStartMusicClip: {fileID: 0}
    - CurrSpeaker: 1
      OptionalNpcReference: {fileID: 11400000, guid: ab87a85575fcd92479d099fd2eccee18,
        type: 2}
      Text: No!
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: 68299baf120e84f4eb3579f5864df440,
        type: 3}
      CustomBackgroundNpcLayout:
        xPos: 0.52
        yPos: 0.42
        width: 375
      OptionalAudioClip: {fileID: 0}
      OptionalStartMusicClip: {fileID: 0}
    - CurrSpeaker: 0
      OptionalNpcReference: {fileID: 11400000, guid: ab87a85575fcd92479d099fd2eccee18,
        type: 2}
      Text: 'She was all excited about the wedding.  Must have slipped her bride
        brain.  '
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: 68299baf120e84f4eb3579f5864df440,
        type: 3}
      CustomBackgroundNpcLayout:
