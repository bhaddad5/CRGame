    Dialogs:
    - CurrSpeaker: 0
      OptionalNpcReference: {fileID: 11400000, guid: 6cf03be3ec0645e47b84af5fbd6d1207,
        type: 2}
      Text: "Sorry, but this can\u2019t wait.  Hey, so you went with Deb\u2019s little
        sister for your PA?  "
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: 68299baf120e84f4eb3579f5864df440,
        type: 3}
      CustomBackgroundNpcLayout:
        xPos: 0.52
