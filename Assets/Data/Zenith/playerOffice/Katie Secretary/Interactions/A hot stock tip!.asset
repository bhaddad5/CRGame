      OptionalNpcReference: {fileID: 11400000, guid: c0796ec1273c4044c909192668fdd801,
        type: 2}
      Text: '{FirstName}, thanks for giving an old man some time.'
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: 68299baf120e84f4eb3579f5864df440,
        type: 3}
      CustomBackgroundNpcLayout:
