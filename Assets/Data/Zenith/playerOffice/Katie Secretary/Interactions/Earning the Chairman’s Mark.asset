    - CurrSpeaker: 1
      OptionalNpcReference: {fileID: 11400000, guid: 6a3d4b7816d4dd24ab8130b1b9178c47,
        type: 2}
      Text: "Debbie didn\u2019t much like her new practice dummy."
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: 68299baf120e84f4eb3579f5864df440,
        type: 3}
      CustomBackgroundNpcLayout:
