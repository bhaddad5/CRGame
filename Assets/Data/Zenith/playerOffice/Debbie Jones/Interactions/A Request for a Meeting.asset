      OptionalAudioClip: {fileID: 0}
      OptionalStartMusicClip: {fileID: 0}
    - CurrSpeaker: 1
      OptionalNpcReference: {fileID: 11400000, guid: ee1da375ae4db704d92072d1a5229c06,
        type: 2}
      Text: "The message from Allison Riordan, requesting a meeting.  It\u2019s at
        the bottom of the pile."
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: 68299baf120e84f4eb3579f5864df440,
        type: 3}
      CustomBackgroundNpcLayout:
        xPos: 0.52
        yPos: 0.42
        width: 375
      OptionalAudioClip: {fileID: 0}
      OptionalStartMusicClip: {fileID: 0}
    - CurrSpeaker: 0
      OptionalNpcReference: {fileID: 11400000, guid: ee1da375ae4db704d92072d1a5229c06,
        type: 2}
      Text: "Oh, that!  Well, your schedule is very busy and it didn\u2019t seem\u2014"
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: 68299baf120e84f4eb3579f5864df440,
        type: 3}
      CustomBackgroundNpcLayout:
        xPos: 0.52
        yPos: 0.42
        width: 375
      OptionalAudioClip: {fileID: 0}
      OptionalStartMusicClip: {fileID: 0}
    - CurrSpeaker: 1
      OptionalNpcReference: {fileID: 11400000, guid: ee1da375ae4db704d92072d1a5229c06,
        type: 2}
      Text: "You don\u2019t like Allison, but that\u2019s no reason to be unprofessional. 
        Just because she succeeded brilliantly and your own executive career failed
        so miserably is no excuse to not to do your job.  Is that clear?"
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: 68299baf120e84f4eb3579f5864df440,
        type: 3}
      CustomBackgroundNpcLayout:
        xPos: 0.52
        yPos: 0.42
        width: 375
      OptionalAudioClip: {fileID: 0}
      OptionalStartMusicClip: {fileID: 0}
    - CurrSpeaker: 0
      OptionalNpcReference: {fileID: 11400000, guid: ee1da375ae4db704d92072d1a5229c06,
        type: 2}
      Text: Crystal clear, Mr. {LastName}.  My apologies for the slip.
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: 68299baf120e84f4eb3579f5864df440,
        type: 3}
      CustomBackgroundNpcLayout:
        xPos: 0.52
        yPos: 0.42
        width: 375
      OptionalAudioClip: {fileID: 0}
      OptionalStartMusicClip: {fileID: 0}
    - CurrSpeaker: 1
      OptionalNpcReference: {fileID: 11400000, guid: ee1da375ae4db704d92072d1a5229c06,
        type: 2}
      Text: 'Set up a meeting with Allison- at her convenience.  And that will be
        a demerit, Debbie.  Well earned I should say.  '
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: 68299baf120e84f4eb3579f5864df440,
        type: 3}
      CustomBackgroundNpcLayout:
        xPos: 0.52
        yPos: 0.42
        width: 375
      OptionalAudioClip: {fileID: 0}
      OptionalStartMusicClip: {fileID: 0}
    - CurrSpeaker: 2
      OptionalNpcReference: {fileID: 11400000, guid: ee1da375ae4db704d92072d1a5229c06,
        type: 2}
      Text: 'Debbie frowns, stung by both the rebuke and the unwelcome demerit! '
