    - CurrSpeaker: 1
      OptionalNpcReference: {fileID: 11400000, guid: ee1da375ae4db704d92072d1a5229c06,
        type: 2}
      Text: "It\u2019s a new training program that includes a course for training
        secretaries to provide a bit more than just typing and filing.  I expect
        you to earn a high grade, Debbie."
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: 7b229387ac624744e8cba637eeda5934,
        type: 3}
      CustomBackgroundNpcLayout:
        xPos: 0.52
        yPos: 0.42
        width: 375
      OptionalAudioClip: {fileID: 0}
      OptionalStartMusicClip: {fileID: 0}
    - CurrSpeaker: 0
      OptionalNpcReference: {fileID: 11400000, guid: ee1da375ae4db704d92072d1a5229c06,
        type: 2}
      Text: I'll try my hardest, Sir.  I want to be more than just an ordinary secretary.
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: 7b229387ac624744e8cba637eeda5934,
        type: 3}
      CustomBackgroundNpcLayout:
