      OptionalAudioClip: {fileID: 8300000, guid: bfa67ddf1dfde624bbcc4cc66244ccdc,
        type: 3}
      OptionalStartMusicClip: {fileID: 0}
    - CurrSpeaker: 0
      OptionalNpcReference: {fileID: 11400000, guid: ee1da375ae4db704d92072d1a5229c06,
        type: 2}
      Text: Yes, Ma'am.
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: 5143e3ce9e33973479a1d001c5e1c665,
        type: 3}
      CustomBackgroundNpcLayout:
        xPos: 0.52
        yPos: 0.42
        width: 375
      OptionalAudioClip: {fileID: 8300000, guid: a13a8664f30e6984cbc95a3bd19bbb37,
        type: 3}
      OptionalStartMusicClip: {fileID: 0}
    - CurrSpeaker: 0
      OptionalNpcReference: {fileID: 11400000, guid: d6893d3d1ae90dc4699580eb2d4b4f1a,
        type: 2}
      Text: Good!  Well, I'm off to tea and shopping with Sally MacAllister.  Kisses,
        Darling!  Ta ta girls!
      CustomNpcImageOptions:
