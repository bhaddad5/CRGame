    - CurrSpeaker: 0
      OptionalNpcReference: {fileID: 11400000, guid: 0041cc9001d9b5e46817e841349ddd12,
        type: 2}
      Text: '{FirstName}, why can''t I remain in my old office?  I''m all set up
        there.  There''s no privacy here!  I''m out in the open!'
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: 936c9fbc30a60ab47a10e29d15a3682d,
        type: 3}
      CustomBackgroundNpcLayout:
        xPos: 0.52
