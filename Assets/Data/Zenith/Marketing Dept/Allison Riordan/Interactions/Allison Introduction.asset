      CustomBackground: {fileID: 0}
      CustomBackgroundNpcLayout:
        xPos: 0
        yPos: 0
        width: 0
    - CurrSpeaker: 1
      OptionalNpcReference: {fileID: 0}
      Text: Thanks.  I'm still finding my way though.
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 0}
      CustomBackgroundNpcLayout:
        xPos: 0
        yPos: 0
        width: 0
    - CurrSpeaker: 0
