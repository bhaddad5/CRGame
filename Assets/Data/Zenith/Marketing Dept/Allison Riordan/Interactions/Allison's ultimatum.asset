      CustomNpcImageOptions: []
      CustomBackground: {fileID: 0}
      CustomBackgroundNpcLayout:
        xPos: 0
        yPos: 0
        width: 0
    - CurrSpeaker: 1
      OptionalNpcReference: {fileID: 0}
      Text: Anything!
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 0}
      CustomBackgroundNpcLayout:
        xPos: 0
        yPos: 0
        width: 0
    - CurrSpeaker: 0
      OptionalNpcReference: {fileID: 0}
      Text: I don't have a secretary of my own.  I'd like to borrow Debbie from time
        to time.
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 0}
      CustomBackgroundNpcLayout:
        xPos: 0
        yPos: 0
        width: 0
    - CurrSpeaker: 2
      OptionalNpcReference: {fileID: 0}
      Text: 'NOTE: Allison is now open to a deeper more meaningful relationship with
