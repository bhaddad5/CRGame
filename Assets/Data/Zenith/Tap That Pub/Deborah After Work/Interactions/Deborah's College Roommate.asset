    - CurrSpeaker: 1
      OptionalNpcReference: {fileID: 0}
      Text: Hi Deborah!  And you are...?
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: e97365762c9b23a42bb7fd06c0e4e40e,
        type: 3}
      CustomBackgroundNpcLayout:
        xPos: 0.5
