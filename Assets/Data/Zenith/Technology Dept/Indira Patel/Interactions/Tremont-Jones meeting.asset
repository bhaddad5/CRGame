      CustomBackgroundNpcLayout:
        xPos: 0
        yPos: 0
        width: 0
      OptionalAudioClip: {fileID: 0}
      OptionalStartMusicClip: {fileID: 0}
    - CurrSpeaker: 2
      OptionalNpcReference: {fileID: 0}
      Text: '"Not only that, I''ll make you my Operations Manager.  You''ll never
        have to deal with {FirstName} {LastName} again-- I''ll make sure {FirstName}
        {LastName} is fired immediately!  Now, can you give me that affidavit?"'
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 0}
      CustomBackgroundNpcLayout:
        xPos: 0
        yPos: 0
        width: 0
      OptionalAudioClip: {fileID: 0}
      OptionalStartMusicClip: {fileID: 0}
    - CurrSpeaker: 2
      OptionalNpcReference: {fileID: 0}
      Text: '"Yes-- I''ll do it!  I can''t wait to see his face!  Me-- a manager! 
        Finally!"  '
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 0}
      CustomBackgroundNpcLayout:
        xPos: 0
