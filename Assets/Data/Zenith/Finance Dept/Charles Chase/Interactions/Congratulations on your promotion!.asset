        width: 350
      OptionalAudioClip: {fileID: 0}
    - CurrSpeaker: 0
      OptionalNpcReference: {fileID: 11400000, guid: 2447b5794cc10d448b75441484c22173,
        type: 2}
      Text: You damn well know you do, {FirstName}!  Whatever you need from Finance
        is yours, boss!
      CustomNpcImageOptions: []
      CustomBackground: {fileID: 2800000, guid: 371197f8242f61c40a9a0ccedaeb75ab,
        type: 3}
      CustomBackgroundNpcLayout:
