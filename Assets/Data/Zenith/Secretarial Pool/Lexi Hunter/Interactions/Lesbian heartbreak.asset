      OptionalNpcReference: {fileID: 11400000, guid: 6a3d4b7816d4dd24ab8130b1b9178c47,
        type: 2}
      Text: Mr. {LastName}, congratulations on your recent nuptials!
      CustomNpcImageOptions: []
